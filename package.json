  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "cloudinary-react": "^1.6.8",
    "core-js": "^3.8.3",
    "gatsby": "^2.32.3",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-postcss": "^2.3.13",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-typography": "^2.12.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-remark": "^2.16.0",
    "graphql": "^14.7.0",
    "moment": "^2.29.1",
    "prismjs": "^1.23.0",
    "prop-types": "15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "upgrade-interactive": "npm-check --update"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "1.6.1",
    "npm-check": "^5.9.2",
    "prettier": "1.18.2",
    "tailwindcss": "1.0.4",
    "typescript": "^3.9.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisjm/chrisjmears-gatsby"
  },
  "bugs": {
    "url": "https://github.com/chrisjm/chrisjmears-gatsby/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
